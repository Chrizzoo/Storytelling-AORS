<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="prettyprint.xsl"?>
<SimulationScenario xmlns="http://aor-simulation.org"
	xmlns:aors="http://aor-simulation.org" xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://aor-simulation.org distribution/ext/aorsl/AORSL-0-8-3.xsd"
	version="0.8.3" simulationManagerDirectory="aors" scenarioName="Juveniles1"
	scenarioTitle="Juveniles Scenario 1">

	<SimulationParameters simulationSteps="10000" />
	
	<SimulationModel modelName="Juveniles1" modelTitle="Juveniles Scenario 1">
		<documentation>
			<dc:creator>Rene Groothedde</dc:creator>
			<dc:created>20130805</dc:created>
			<dc:contributor>Chris Borkema</dc:contributor>
		</documentation>
		<aors:SpaceModel >
			<aors:TwoDimensional yMax="100" xMax="100" ></aors:TwoDimensional>
		</aors:SpaceModel>

		<EntityTypes>

			<aors:PhysicalAgentType name="Human" ></aors:PhysicalAgentType>
			<aors:PhysicalAgentType name="Police"
				superType="Human"></aors:PhysicalAgentType>
			<aors:PhysicalAgentType name="Juvenile"
				superType="Human" ></aors:PhysicalAgentType>
		</EntityTypes>

	</SimulationModel>

	<InitialState>
		<aors:PhysicalAgent type="Police" name="police" id="1" x="5" y="5"></aors:PhysicalAgent>
		<aors:PhysicalAgent type="Juvenile" name="henk" id="2"></aors:PhysicalAgent>

	</InitialState>

	<UserInterface>
		<aors:AnimationUI>
			<aors:Views>
				<aors:PhysicalObjectView physicalObjectType="Human" physicalObjectIdRef="1"><aors:DisplayInfo  property="name"></aors:DisplayInfo></aors:PhysicalObjectView>
			</aors:Views>
		</aors:AnimationUI>
	</UserInterface>

</SimulationScenario>
